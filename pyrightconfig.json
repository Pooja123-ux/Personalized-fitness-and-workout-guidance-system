{
  "pythonVersion": "3.13",
  "pythonPath": "${workspaceRoot}",
  "typeCheckingMode": "basic",
  "include": [
    "${workspaceRoot}/backend",
    "${workspaceRoot}/app",
    "${workspaceRoot}/test_*.py"
  ],
  "exclude": [
